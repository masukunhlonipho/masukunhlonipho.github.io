{
  "extends": [
    "development"
  ],
  "hints": {
    "disown-opener": "off",
    "axe/structure": [
      "default",
      {
        "listitem": "off",
        "list": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/aria": [
      "default",
      {
        "aria-required-parent": "off",
        "aria-valid-attr-value": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "@-ms-viewport",
          "@-ms-viewport",
          "@-ms-viewport",
          "@-ms-viewport"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 122",
    "not and_ff <= 123",
    "not chrome <= 122",
    "not edge <= 122",
    "not firefox <= 123",
    "not ios_saf <= 17.4",
    "not opera <= 108",
    "not safari <= 17.4",
    "not samsung <= 23"
  ]
}